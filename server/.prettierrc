{
    "semi": true,
    "trailingComma": "none",
    "singleQuote": true,
    "printWidth": 300,
    "tabWidth": 4,
    "useTabs": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "proseWrap": "preserve"
}
